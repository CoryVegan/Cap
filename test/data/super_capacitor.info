type SuperCapacitor
dim  2

geometry {
    mesh_file mesh_2d.ucd

    anode_collector_width    5.0e-6 ; [meter]
    anode_electrode_width   50.0e-6 ; [meter]
    separator_width         25.0e-6 ; [meter]
    cathode_electrode_width 50.0e-6 ; [meter]
    cathode_collector_width  5.0e-6 ; [meter]
    sandwich_height         25.0e-6 ; [meter]
    sandwich_depth          50.0e-6 ; [meter]
    tab_height               5.0e-6 ; [meter]

    anode_collector_material_id   4
    anode_electrode_material_id   1
    separator_material_id         2
    cathode_electrode_material_id 3
    cathode_collector_material_id 5

    materials 4
    material_0 {    
        name            anode
        material_id         1
    }
    material_1 {
        name        separator
        material_id         2
    }
    material_2 {
        name          cathode
        material_id         3
    }
    material_3 {
        name        collector
        material_id       4,5
    }
}

temperature_component      0
solid_potential_component  1
liquid_potential_component 2
thermal_block         0
electrochemical_block 1
n_blocks              2

material_properties {
    anode {
        type           porous_electrode
        matrix_phase   electrode_material
        solution_phase electrolyte
    }
    cathode {
        type           porous_electrode
        matrix_phase   electrode_material
        solution_phase electrolyte
    }
    separator {
        type           permeable_membrane
        matrix_phase   separator_material
        solution_phase electrolyte
    }
    collector {
        type           current_collector
        metal_foil     collector_material
    }

    separator_material {
        void_volume_fraction             0.6       ;
        tortuosity_factor                1.29      ;
        pores_characteristic_dimension   1.5e-9    ; [meter]
        pores_geometry_factor            2.0       ;
        mass_density                     3.2e3     ; [kilogram per cubic meter]
        heat_capacity                    1.2528e3  ; [joule per kilogram kelvin]
        thermal_conductivity             0.0019e2  ; [watt per meter kelvin]
    }
    electrode_material {
        differential_capacitance         0.03134   ; [farad per square meter]
        exchange_current_density         0.0       ; [ampere per square meter]
        void_volume_fraction             0.67      ; 
        tortuosity_factor                2.3       ; 
        pores_characteristic_dimension   1.5e-9    ; [meter]
        pores_geometry_factor            2.0       ; 
        mass_density                     2.3e3     ; [kilogram per cubic meter]
        electrical_conductivity         52.1       ; [siemens per meter]
        heat_capacity                    0.93e3    ; [joule per kilogram kelvin]
        thermal_conductivity             0.0011e2  ; [watt per meter kelvin]
    }
    collector_material {
        mass_density                     2.7e3     ; [kilogram per cubic meter]
        electrical_resistivity          28.2e-9    ; [ohm meter]
        heat_capacity                    2.7e3     ; [joule per kilogram kelvin]
        thermal_conductivity           237.0       ; [watt per meter kelvin]
    }
    electrolyte {
        mass_density                     1.2e3     ; [kilogram per cubic meter]
        conductivity                     0.067     ; [siemens per meter]
        heat_capacity                    0.0       ; [joule per kilogram kelvin]
        thermal_conductivity             0.0       ; [watt per meter kelvin]
    }
}

boundary_values {
    anode_boundary_id   1
    cathode_boundary_id 2

    ambient_temperature                300.0
    heat_transfer_coefficient            8.0e-2

    upper_boundary_id             3
    lower_boundary_id             4
    other_boundary_id             5

    separator_material_id         3
    anode_electrode_material_id   4
    anode_collector_material_id   5
    cathode_electrode_material_id 6
    cathode_collector_material_id 7
}
