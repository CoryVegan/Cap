<impedance_spectroscopy>
    <mode>potentiostatic</mode>
    <frequency_upper_limit>    1.0e+3 </frequency_upper_limit> <!-- hertz -->
    <frequency_lower_limit>    1.0e-4 </frequency_lower_limit> <!-- hertz -->
    <steps_per_decade>         6      </steps_per_decade>

    <cycles>                   5      </cycles>
    <ignore_cycles>            4      </ignore_cycles>
    <steps_per_cycle>       1024      </steps_per_cycle>

    <dc_voltage>1.1</dc_voltage> <!-- volt -->
    <harmonics>3,5</harmonics>
    <amplitudes>5.0e-3,5.0e-3</amplitudes>
    <phases>0.0,30.0</phases> <!-- degree -->
</impedance_spectroscopy>

